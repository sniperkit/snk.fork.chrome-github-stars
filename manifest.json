{
  "manifest_version": 2,
  "name": "GithubStars",
  "version": "0.1",
  "description": "Displays stars with github links",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "handler.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
        //"content_script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
